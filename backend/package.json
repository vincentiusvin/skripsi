{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha",
    "test:external": "mocha --config .mocharc.external.json",
    "test:watch": "nodemon --exec mocha",
    "test:cov": "c8 mocha",
    "start": "nodemon --exec tsx --env-file=../.env src/index.ts",
    "start:prod": "node dist/migrate.js latest && node dist/index.js",
    "build": "tsc",
    "db:int:generate-types": "kysely-codegen --env-file=../.env --out-file ./src/db/db_types.d.ts",
    "db:int:migrate": "tsx --env-file=../.env src/migrate.ts",
    "db:dummy": "tsx --env-file=../.env src/dummy.ts",
    "db:latest": "npm run db:int:migrate latest && npm run db:int:generate-types",
    "db:init": "npm run db:int:migrate init && npm run db:int:generate-types"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-pg-simple": "^9.0.1",
    "dayjs": "^1.11.13",
    "express": "^5.0.0",
    "express-session": "^1.18.0",
    "kysely": "^0.27.4",
    "kysely-codegen": "^0.14.2",
    "pg": "^8.12.0",
    "socket.io": "^4.7.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^4.3.19",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.5.4",
    "@types/pg": "^8.11.9",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "eslint": "^8.57.0",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.4",
    "typescript-eslint": "^7.18.0"
  }
}
